@model IEnumerable<RemliCMS.WebData.Entities.PageIndex>

@foreach (var item in Model)

{
        Html.RenderAction("PageContent", "Home", new {pageIndexId = item.Id, translationId = ViewBag.TranslationId});
}


@section head
{
    @if (ViewBag.isAdmin == false)
    {
        Html.RenderAction("NavBar", "Shared", new { currentPermalink = ViewBag.Permalink });
    }
    else
    {
        Html.RenderAction("AdminHeader", "Shared", new { currentItem = "registration" });
    }
}
@section routedebug
{
    @if(ViewBag.Debug == true) {
        {Html.RenderPartial("RouteDebug");}
    }
    
}